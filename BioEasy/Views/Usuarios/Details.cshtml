@model BioEasy.Data.Entities.Usuario

@{
    ViewData["Title"] = "Detalhes do Usuário";
}

<h1>Detalhes do Usuário</h1>

<div>
    <h4>Usuário</h4>
    <hr />
    <dl class="row">
